			var output="";
			while(grRecord.next()){
				output+="<tr><td><b>Name:</b>"+grRecord.name+"<br /><b>Title:</b>"+grRecord.title+"<br /><b>Phone Number:</b>"+grRecord.mobile_phone+"<br /><b>Email:</b>"+grRecord.email+"</td>";
